<%- include ("partials/header") %>

<div class="container">
  <div class="row">
    <!-- formulario -->
    <div class="col-md-5">
      <div class="card card-body">
        <form action="">
          <div class="form-group">
            <input
              type="text"
              name="title"
              placeholder="Inserte un titulo"
              class="form-control"
              autofocus
            />
          </div>
          <div class="form-group">
            <textarea
              name="description"
              cols="30"
              placeholder="Ingrese una descripción"
              class="form-control"
            ></textarea>
          </div>
          <button type="submit" class="btn btn-primary btn-block">
            Enviar
          </button>
        </form>
      </div>
    </div>
    <!-- listado -->
    <div class="col-md-7">
      <table class="table table-hover table-bordered">
        <thead>
          <tr>
            <th>N°</th>
            <th>Title</th>
            <th>Description</th>
            <th>Operations</th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  </div>
</div>

<%- include ("partials/footer") %>
